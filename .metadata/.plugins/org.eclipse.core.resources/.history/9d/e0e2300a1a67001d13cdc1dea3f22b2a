package entrainement;

public class MainClass {
	public static void main(String[] args) {
		Member MemMatoya = new Member("Rhul", "Y'Shtola", "30", "Magie");
		
		String texte = "Bonjour, je me nomme " + MemMatoya.getFirstName() + " " + MemMatoya.getName() + " et j'ai " + MemMatoya.getAge() + " ans. J'étudie actuellement dans la section " + MemMatoya.getSection() + ". Enchantée.";
		
		System.out.println(texte);
		
	}

}
