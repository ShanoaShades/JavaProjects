package exercice1;

import javax.swing.*;

class TextField {
	public static void main(String args[]) {
		JFrame f = new JFrame("TextField Simple");
		// On crée les objets t1, t2, t3
		JTextField t1, t2, t3;
		t1 = new JTextField("Welcome to Javatpoint.");
		t1.setBounds(50, 100, 200, 30);
		t2 = new JTextField("AWT Tutorial");
		t2.setBounds(50, 150, 200, 30);
		t3 = new JTextField("Et ça, c'est de moi !");
		t3.setBounds(50, 200, 200, 30);
		f.add(t1);
		f.add(t2);
		f.add(t3);
		f.setSize(400, 400);
		f.setLayout(null);
		f.setVisible(true);
	}
}